                      %>
                      <div class="alert alert-success text-white">
                        <%=messages %>
                      </div>
                      <%
                  }
                %>
               <% if(status == 0){   %>
                  <div class="alert alert-danger text-white">
                      <button type="button" class="close ml-auto" data-dismiss="alert" aria-label="Close"> <span aria-hidden="true" class="text-white">Ã—</span> </button>
                      <h5 class="text-white"> Alert</h5> 
                      <ul>
                          <% Object.values(message).forEach(function(error){ %>
                                <li><%= error.msg %></li>
                          <% }); %>
                      </ul>
                    
                  </div>
                <%  } %>
